int subarraysDivByK(int* nums, int numsSize, int k) {
    int* modCount = (int*)calloc(k, sizeof(int));
    modCount[0] = 1;
    int prefixSum = 0, count = 0;
    for (int i = 0; i < numsSize; i++)
     {
     prefixSum += nums[i];
     int mod = ((prefixSum % k) + k) % k; 
     count += modCount[mod];
     modCount[mod]++;
    }
    free(modCount);
    return count;
}
