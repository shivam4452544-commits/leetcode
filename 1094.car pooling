#include <stdbool.h> 
#include <stdio.h.>
bool carPooling(int capacity, int trips[][3], int numTrips) { 
    int passengerChanges[1001] = {0}; 
    for (int i = 0; i < numTrips; i++) {
        int numPassengers = trips[i][0];
        int from = trips[i][1];
        int to = trips[i][2];
        passengerChanges[from] += numPassengers;  
        passengerChanges[to] -= numPassengers;   
    }
    int currentPassengers = 0;
    for (int i = 0; i < 1001; i++) { 
        currentPassengers += passengerChanges[i];
        if (currentPassengers > capacity) {
            return false; 
        }
    }
    return true; 
}
