int partitionDisjoint(int* nums, int numsSize) {
    int left_arr = nums[0];
    int max_left = nums[0];
    int index = 0;

    for (int i = 1 ; i< numsSize ; i++){
        if (nums[i]<left_arr){
            index = i;
            left_arr = max_left;
        }
        else if (nums[i]>max_left){
            max_left = nums[i];
        }
    }
    return index+1;
    
}
